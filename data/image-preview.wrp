<!DOCTYPE report [
  <!ELEMENT report (page)*>
  <!ELEMENT page (row|col|text|image|svg|vline|hline)*>
  <!ATTLIST page
            name CDATA #IMPLIED
            child_layout (col|row) "col">
  <!ELEMENT row (row|col|text|image|svg|vline|hline)*>
  <!ATTLIST row
            name CDATA #IMPLIED
            margins CDATA "(0,0,0,0)"
            spacing CDATA "0">
  <!ELEMENT col (row|col|text|image|svg|vline|hline)*>
